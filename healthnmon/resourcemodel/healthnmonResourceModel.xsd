    <!-- ++++++++++++++++++++++++++++++ Common Model ++++++++++++++++++++++++++++++ -->
    <!-- Base Type Definitions -->
    <xs:complexType name="Entity">
        <xs:annotation>
            <xs:appinfo>
                <hj:mapped-superclass />
            </xs:appinfo>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="id" type="xs:string" minOccurs="1" maxOccurs="1"> <!-- ISC unique ID for an object. Set by the owner/creator of the object. -->
                <xs:annotation>
                    <xs:appinfo>
                        <hj:id />
                    </xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="name" type="xs:string" minOccurs="0" maxOccurs="1" />
            <xs:element name="note" type="xs:string" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <annox:annotate>
                            <jxp:Lob />
                        </annox:annotate>
                    </xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="createEpoch" type="xs:long" minOccurs="0" maxOccurs="1" />
            <xs:element name="lastModifiedEpoch" type="xs:long" minOccurs="0" maxOccurs="1" />
            <xs:element name="deletedEpoch" type="xs:long" minOccurs="0" maxOccurs="1" />
            <xs:element name="deleted" type="xs:boolean" minOccurs="0" maxOccurs="1" />
            <xs:element name="cost" type="Cost" minOccurs="0" maxOccurs="1" />
            <xs:element name="properties" type="Property" minOccurs="0" maxOccurs="unbounded" /> <!-- Name=value properties, allows adding additional attributes to objects without formally changing the model. -->
        </xs:sequence>
    </xs:complexType>

    <!-- Inherited Type Definitions -->

    <xs:complexType name="Property" mixed="false"> <!-- used by Service, Resource, and Management models. Contained in Entity. -->
        <xs:sequence>
            <xs:element name="id" type="xs:integer" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo>
                        <hj:id />
                    </xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element name="name" type="xs:string" minOccurs="1" maxOccurs="1" />
            <xs:element name="note" type="xs:string" minOccurs="0" maxOccurs="1" />
            <xs:element name="value" type="xs:string" minOccurs="1" maxOccurs="1" />
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="Cost"> <!-- used by Service, Resource, and Management models. Contained in Entity. -->
        <xs:annotation>
            <xs:appinfo>
                <hj:embeddable />
            </xs:appinfo>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="value" type="xs:decimal" minOccurs="0" maxOccurs="1" />
            <xs:element name="units" type="xs:string" minOccurs="0" maxOccurs="1" />
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="ResourceTag">
        <xs:sequence>
            <xs:element name="value" type="xs:string" minOccurs="1" maxOccurs="1" />
            <xs:element name="assertionType" type="xs:string" minOccurs="1" maxOccurs="1" />
        </xs:sequence>
    </xs:complexType>

