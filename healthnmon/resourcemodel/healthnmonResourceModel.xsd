    <!-- Base Type Definitions -->
    <xs:complexType name="Resource" mixed="false">
        <xs:complexContent mixed="false">
            <xs:extension base="Entity"> <!-- ISC Note: Entity has expanded list of attributes that can be populated on Resources. -->
                <xs:sequence>
                    <!-- Properties -->
                    <xs:element name="resourceManagerId" type="xs:string" minOccurs="0" maxOccurs="1" />
                    <!-- For Resources, the Entity.Id may not be globally unique in ISC, if mutliple resource managers of the the same 
                        type are integrated into ISC. The combination of Entity.Id and Resource.ResourceManagerId will be unique. -->

                    <xs:element name="utilization" type="ResourceUtilization" minOccurs="0" maxOccurs="1" />
                    <xs:element name="limits" type="ResourceLimit" minOccurs="0" maxOccurs="1" />

                    <!-- Additional, vendor specific properties -->
                    <xs:element name="vendorProperties" type="VendorProperties" minOccurs="0" maxOccurs="1" />

                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="ResourceUtilization">
        <!-- Resource utilization metrics in bytes or percentages -->
        <xs:sequence>
            <xs:element name="resourceId" type="xs:string" minOccurs="0" maxOccurs="1" />
            
            <xs:element name="timestamp" type="xs:dateTime" minOccurs="0" maxOccurs="1" />
            <xs:element name="granularity" type="xs:int" minOccurs="0" maxOccurs="1" />
            
            <xs:element name="cpuUserLoad" type="xs:double" minOccurs="0" maxOccurs="1" />
            <xs:element name="cpuSystemLoad" type="xs:double" minOccurs="0" maxOccurs="1" />
            
            <xs:element name="hostCpuSpeed" type="xs:long" minOccurs="0" maxOccurs="1" />
            <xs:element name="hostMaxCpuSpeed" type="xs:long" minOccurs="0" maxOccurs="1" />
            <xs:element name="ncpus" type="xs:int" minOccurs="0" maxOccurs="1" />
            
            <xs:element name="diskRead" type="xs:double" minOccurs="0" maxOccurs="1" />
            <xs:element name="diskWrite" type="xs:double" minOccurs="0" maxOccurs="1" />
            
            <xs:element name="netRead" type="xs:double" minOccurs="0" maxOccurs="1" />
            <xs:element name="netWrite" type="xs:double" minOccurs="0" maxOccurs="1" />
            
            <xs:element name="totalMemory" type="xs:long" minOccurs="0" maxOccurs="1" />
            <xs:element name="freeMemory" type="xs:long" minOccurs="0" maxOccurs="1" />
            <xs:element name="configuredMemory" type="xs:long" minOccurs="0" maxOccurs="1" />
            
            <xs:element name="uptimeMinute" type="xs:long" minOccurs="0" maxOccurs="1" />
            
            <xs:element name="reservedSystemCapacity" type="xs:long" minOccurs="0" maxOccurs="1" />
            <xs:element name="maximumSystemCapacity" type="xs:long" minOccurs="0" maxOccurs="1" />
            <xs:element name="relativeWeight" type="xs:long" minOccurs="0" maxOccurs="1" />
            
            <xs:element name="reservedSystemMemory" type="xs:long" minOccurs="0" maxOccurs="1" />
            <xs:element name="maximumSystemMemory" type="xs:long" minOccurs="0" maxOccurs="1" />
            <xs:element name="memoryRelativeWeight" type="xs:long" minOccurs="0" maxOccurs="1" />
            <xs:element name="status" type="xs:int" minOccurs="0" maxOccurs="1" />
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="ResourceLimit">
        <!-- Resource limitations or quotas in bytes or percentages -->
        <xs:sequence>
            <xs:element name="resourceId" type="xs:string" minOccurs="0" maxOccurs="1" />
            <xs:element name="memoryConsumedLimit" type="xs:long" minOccurs="0" maxOccurs="1" />
            <xs:element name="memoryPercentLimit" type="xs:long" minOccurs="0" maxOccurs="1" />
            <xs:element name="diskSpaceConsumedLimit" type="xs:long" minOccurs="0" maxOccurs="1" />
            <xs:element name="diskSpacePercentLimit" type="xs:long" minOccurs="0" maxOccurs="1" />
            <xs:element name="cpuPercentLimit" type="xs:long" minOccurs="0" maxOccurs="1" />
            <xs:element name="networkPercentLimit" type="xs:long" minOccurs="0" maxOccurs="1" />
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="VendorProperties" mixed="false">
        <!-- An empty container designed to hold Property elements defining vendor specific attributes on a resource. -->
        <xs:complexContent mixed="false">
            <xs:extension base="Entity">
                <xs:sequence>
                    <!-- Properties -->

                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

