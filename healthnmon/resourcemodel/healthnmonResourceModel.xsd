                    <xs:element name="autoStopAction" type="xs:string" minOccurs="0" maxOccurs="1" />
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="OsProfile" mixed="false">
        <!-- Common element for the OS attributes of running virtual and physical servers. -->
        <xs:sequence>
            <!-- Properties -->
            <xs:element name="resourceId" type="xs:string" minOccurs="0" maxOccurs="1" />
            <xs:element name="osType" type="OsTypeEnum" minOccurs="0" maxOccurs="1" />
            <xs:element name="osSubType" type="xs:string" minOccurs="0" maxOccurs="1" />
            <xs:element name="osDescription" type="xs:string" minOccurs="0" maxOccurs="1" />
            <xs:element name="osName" type="xs:string" minOccurs="0" maxOccurs="1" />
            <xs:element name="osVersion" type="xs:string" minOccurs="0" maxOccurs="1" /> <!-- What is Group ID? VCDG for PhysicalServer, Cluster ID for VMHost? -->
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="IpProfile" mixed="false">
        <!-- Common element for the IP attributes of running virtual and physical servers. -->
        <xs:sequence>
            <!-- Properties -->
            <xs:element name="ipAddress" type="xs:string" minOccurs="0" maxOccurs="1" />
            <xs:element name="ipType" type="IpTypeEnum" minOccurs="0" maxOccurs="1" />
            <xs:element name="hostname" type="xs:string" minOccurs="0" maxOccurs="1" />
            <xs:element name="domain" type="xs:string" minOccurs="0" maxOccurs="1" />
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="VmNetAdapter" mixed="false">
        <!-- Common element for the Network attributes of running virtual servers. -->
        <xs:complexContent mixed="false">
            <xs:extension base="Resource">
                <xs:sequence>
                    <!-- Properties -->
                    <xs:element name="addressType" type="xs:string" minOccurs="0" maxOccurs="1" /><!-- MAC address type Automatic 
                        or Manual -->
                    <xs:element name="adapterType" type="xs:string" minOccurs="0" maxOccurs="1" /><!-- Adapter TYpe like E1000, 
                        VMXNET, Legacy, Synthetic -->
                    <xs:element name="switchType" type="xs:string" minOccurs="0" maxOccurs="1" /><!-- Switch Type like vSwitch, 
                        dvSwitch -->
                    <xs:element name="macAddress" type="xs:string" minOccurs="0" maxOccurs="1" />
                    <xs:element name="ipAddresses" type="xs:string" minOccurs="0" maxOccurs="unbounded" />
                    <xs:element name="networkName" type="xs:string" minOccurs="0" maxOccurs="1" /><!-- Virtual Switch Name 
                        in case of Hyper-V / PortGroup Name in case of ESX -->
                    <xs:element name="vlanId" type="xs:string" minOccurs="0" maxOccurs="1" />
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="VmScsiController" mixed="false">
        <!-- Common element for the Network attributes of running virtual servers. -->
        <xs:complexContent mixed="false">
            <xs:extension base="Resource">
                <xs:sequence>
                    <!-- Properties -->
                    <xs:element name="controllerId" type="xs:int" minOccurs="0" maxOccurs="1" /><!-- Controller number -->
                    <xs:element name="controllerName" type="xs:string" minOccurs="0" maxOccurs="1" /><!-- Display name of 
                        the controller like "SCSI Controller 1" -->
                    <xs:element name="type" type="xs:string" minOccurs="0" maxOccurs="1" /><!-- Controller Type like lsiLogic, 
                        busLogic, lsiLogicSAS, paraVirtual -->
                    <xs:element name="busSharing" type="xs:string" minOccurs="0" maxOccurs="1" /><!-- BusSharing details like 
                        none, physical, virtual -->
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="VmDisk" mixed="false">
        <xs:complexContent mixed="false">
            <xs:extension base="Resource">
                <xs:sequence>
                    <!-- Properties -->
